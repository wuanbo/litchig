{
    "name": "litchig/asset",
    "authors": [
        {
            "name": "Jing Li",
            "email": "lym125@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "laravel/framework": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Litchig\\Asset\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Litchig\\Asset\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Litchig\\Asset\\AssetServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
